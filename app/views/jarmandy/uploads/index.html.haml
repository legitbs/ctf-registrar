%h1 Listing uploads

%table
  %thead
    %tr
      %th User
      %th Challenge
      %th File
      %th
      %th
      %th

  %tbody
    - @uploads.each do |upload|
      %tr
        %td= upload.user
        %td= upload.challenge
        %td= upload.file
        %td= link_to 'Show', upload
        %td= link_to 'Edit', edit_upload_path(upload)
        %td= link_to 'Destroy', upload, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Upload', new_upload_path
